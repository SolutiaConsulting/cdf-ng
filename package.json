{
  "name": "@cdf/cdf-ng",
  "version": "2.0.8",
  "author": "Tom Schreck <tom_schreck@solutiaconsulting.com> (http://solutiaconsulting.com)",
  "homepage": "https://github.com/SolutiaConsulting/cdf-ng#readme",
  "license": "MIT",
  "description": "Content Delivery Framework for Angular",
  "main": "./src/index.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pree2e": "webdriver-manager update",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "prepublish": "tsc -p ./tsconfig.publish.json",
    "clean:aot": "rimraf aot/*",
    "clean:lib": "rimraf lib/*",
    "clean:lib-temp": "rimraf lib-temp/*",
    "clean": "npm run clean:aot && npm run clean:lib && npm run clean:lib-temp",
    "copy:assets": "cpx \"src/assets/**/*\" lib/assets",
    "copy:src": "cpx \"lib-temp/src/**/*\" lib",
    "copy": "npm run copy:assets && npm run copy:src",
    "ngc": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "b": "npm run clean && npm run ngc && npm run copy && npm run clean:lib-temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SolutiaConsulting/cdf-ng.git"
  },
  "bugs": {
    "url": "https://github.com/SolutiaConsulting/cdf-ng/issues"
  },
  "typings": "./lib/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6",
    "@angular/cli": "1.0.0-rc.1",
    "@angular/compiler-cli": "^2.4.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.0.0",
    "@angular/platform-server": "^2.4.7",
    "ts-helpers": "^1.1.1",
    "rimraf": "^2.5.4",
    "cpx": "^1.5.0"
  },
  "keywords": [
    "Angular",
    "REST",
    "Content",
    "Delivery",
    "Framework"
  ]
}
